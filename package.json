  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/call-hooks.git"
  },
  "keywords": [
    "call",
    "hooks",
    "function",
    "override",
    "intercept"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.4.1",
    "ava-spec": "^1.1.1",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5",
    "typescript-transform-export-interop": "^1.0.2"
  }
